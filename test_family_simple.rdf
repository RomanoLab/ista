<?xml version="1.0" encoding="UTF-8"?>
<rdf:RDF
    xmlns:fam="http://example.org/family#"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:xml="http://www.w3.org/XML/1998/namespace">

    <owl:Ontology rdf:about="http://example.org/family"/>

    <owl:Class rdf:about="http://example.org/family#Person"/>

    <owl:Class rdf:about="http://example.org/family#Parent"/>

    <owl:ObjectProperty rdf:about="http://example.org/family#hasChild"/>

    <owl:DatatypeProperty rdf:about="http://example.org/family#age"/>

    <owl:NamedIndividual rdf:about="http://example.org/family#John"/>

    <owl:NamedIndividual rdf:about="http://example.org/family#Mary"/>

    <owl:Class rdf:about="http://example.org/family#Parent">
        <rdfs:subClassOf rdf:resource="http://example.org/family#Person"/>
    </owl:Class>

    <owl:NamedIndividual rdf:about="http://example.org/family#John">
        <rdf:type rdf:resource="http://example.org/family#Person"/>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="http://example.org/family#Mary">
        <rdf:type rdf:resource="http://example.org/family#Parent"/>
    </owl:NamedIndividual>

    <owl:ObjectProperty rdf:about="http://example.org/family#hasChild">
        <rdfs:domain rdf:resource="http://example.org/family#Person"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="http://example.org/family#hasChild">
        <rdfs:range rdf:resource="http://example.org/family#Person"/>
    </owl:ObjectProperty>

    <owl:DatatypeProperty rdf:about="http://example.org/family#age">
        <rdfs:domain rdf:resource="http://example.org/family#Person"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="http://example.org/family#age">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    </owl:DatatypeProperty>

</rdf:RDF>
